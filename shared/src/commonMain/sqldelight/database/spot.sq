
CREATE TABLE spotEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    desc TEXT NOT NULL
);

getAllSpots:
SELECT *
FROM spotEntity;

getSpotById:
SELECT *
FROM spotEntity
WHERE id = ?;

insertSpot:
INSERT OR REPLACE
INTO spotEntity(
id,
title,
desc
) VALUES (?,?,?);

deleteSpot:
DELETE FROM spotEntity
WHERE id = ?;